<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="hintText" format="string" />
    <attr name="titleText" format="string" />
    <attr name="swChecked" format="boolean" />
    <attr name="expandedTitleText" format="string" />

    <declare-styleable name="SearchBarView">
        <attr name="hintText"/>
    </declare-styleable>

    <declare-styleable name="HeaderView">
        <attr name="titleText"/>
        <attr name="expandedTitleText"/>
    </declare-styleable>

    <declare-styleable name="FunctionalButton">
        <attr name="titleText"/>
        <attr name="swChecked"/>
        <attr name="type" format="enum">
            <enum name="button" value="0"/>
            <enum name="_switch" value="1"/>
        </attr>
    </declare-styleable>

</resources>